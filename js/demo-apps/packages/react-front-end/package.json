{
  "name": "@lunasec/react-front-end",
  "private": true,
  "version": "1.0.7",
  "license": "Apache-2.0",
  "scripts": {
    "start": "yarn run react-scripts start",
    "build": "yarn run react-scripts build",
    "serve-static": "yarn run serve -s build",
    "test": "yarn run react-scripts test",
    "test:url": "./get-application-url.sh",
    "test:e2e": "yarn run cypress run --browser chrome ${TEST_OPTIONS:-''} --config baseUrl=$(yarn run --silent test:url)",
    "test:e2e:docker": "Xvfb :99 & TEST_OPTIONS=\"--headed\" yarn run test:e2e",
    "test:open": "yarn run cypress open --browser chrome --config baseUrl=$(yarn run --silent test:url)",
    "eject": "yarn dlx react-scripts eject",
    "prettier": "yarn run prettier \"*.{js,json,yml,md}\" \"src/**/*\"",
    "format": "yarn run lint --fix",
    "format:check": "yarn run lint",
    "lint": "yarn run eslint --ext .js,.jsx,.ts,.tsx ./src"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@aws-sdk/credential-provider-ini": "^3.31.0",
    "@lunasec/browser-common": "^1.0.7",
    "@lunasec/demo-back-end": "^1.0.7",
    "@lunasec/react-sdk": "^1.0.7",
    "@lunasec/tokenizer-sdk": "^1.0.7",
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "axios": "^0.24.0",
    "cypress-log-to-output": "^1.1.2",
    "cypress-terminal-report": "^3.4.0",
    "easy-peasy": "^5.0.3",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.12.5",
    "husky": "^6.0.0",
    "prettier": "^2.2.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "rimraf": "^3.0.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.11.1",
    "@testing-library/jest-dom": "^5.15.1",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/cypress": "^1.1.3",
    "@types/jest": "^27.0.3",
    "@types/node": "^14.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.8",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "cypress": "9.1.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-iframe": "^1.0.1",
    "eslint": "^8.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-import-order": "^2.1.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^5.2.0",
    "eslint-plugin-react": "^7.27.1",
    "jest": "27.4.3",
    "lerna": "^3.22.1",
    "lint-staged": "^10.5.4",
    "serve": "^11.3.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.3"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
